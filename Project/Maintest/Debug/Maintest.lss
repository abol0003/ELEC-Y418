                                 ClearOldPos:
                                     ; Clears the previous position of the snake by resetting the corresponding pixel in the buffer while keeping obstacles intact.
0000a4 93cf                      	PUSH YL
0000a5 93df                      	PUSH YH
0000a6 d014                          RCALL GetByteAndMask ;R0 contains the current buffer byte, R1 the pixel mask
0000a7 9410                      	COM R1  ; allow to keep the obstacles on when snake goes in same byte of an pixel of obstacle
0000a8 2001                      	AND R0,R1
0000a9 8208                      	ST Y, R0
0000aa 91cf                      	POP YL
0000ab 91df                      	POP YH
0000ac 9508                      	RET
                                 
                                 SetPosBuffer:
                                     ; Sets the position of the snake by updating the corresponding pixel in the display buffer, accounting for obstacles.
0000ad 93cf                          PUSH YL
0000ae 93df                          PUSH YH
0000af 920f                      	PUSH R0
0000b0 921f                      	PUSH R1
0000b1 d009                          RCALL GetByteAndMask 
0000b2 d0b4                      	RCALL CheckObstacles  
0000b3 d007                      	RCALL GetByteAndMask ; we have to call it 2 times because if new food is in same byte, the food will not be showed as Checkobstacle also change buffer
0000b4 2801                          OR R0, R1            ; do or to keep previous led on on ( for example obstacle etc)
0000b5 8208                          ST Y, R0 
0000b6 900f                      	POP R0
0000b7 901f                      	POP R1            
0000b8 91df                          POP YH
0000b9 91cf                          POP YL
0000ba 9508                          RET
                                 GetByteAndMask:
                                     ; Retrieves the current byte and calculates the mask for the snake's position based on its row and column, returning the value from the buffer.
0000bb 930f                          PUSH R16
0000bc 922f                          PUSH R2
0000bd 923f                          PUSH R3
0000be 2e24                          MOV R2, R20
0000bf 2e35                          MOV R3, R21
0000c0 e0c0                          LDI YL, low(0x0100)
0000c1 e0d1                          LDI YH, high(0x0100)
                                 GetByteAndMaskRow:
0000c2 2022                          TST R2
0000c3 f019                          BREQ GetByteAndMaskP2 ; branch if R2=0
0000c4 9625                          ADIW Y, 5 ; enable to select the good row
0000c5 942a                          DEC R2
0000c6 cffb                          RJMP GetByteAndMaskRow
                                 GetByteAndMaskP2:
0000c7 e008                          LDI R16, 8    ; Chaque octet représente 8 colonnes
                                 GetByteAndMaskCol:
0000c8 1630                          CP R3, R16 ; is doing colnum/8
0000c9 f018                          BRLO GetByteAndMaskP3	;branch if lower
0000ca 1a30                          SUB R3, R16
0000cb 9621                          ADIW Y, 1 ; max 5 times as only 5 octet in a row
0000cc cffb                          RJMP GetByteAndMaskCol
                                 GetByteAndMaskP3:; here we are in the right address of the ram
0000cd e001                          LDI R16, 0b00000001  ; Initial mask for column 0
                                 GetByteAndMaskColMask:
0000ce 2033                          TST R3 ; here the remainder <8 will determine which column you are in 
0000cf f019                          BREQ GetByteAndMaskEnd
0000d0 0f00                          LSL R16             ; Shift to the left because a column 0 is on the far right
0000d1 943a                          DEC R3
0000d2 cffb                          RJMP GetByteAndMaskColMask
                                 GetByteAndMaskEnd:
0000d3 8008                          LD R0, Y            ;  reads the contents of the memory pointed to by Y
0000d4 2e10                          MOV R1, R16         
0000d5 903f                          POP R3
0000d6 902f                          POP R2
0000d7 910f                          POP R16
0000d8 9508                          RET
                                 
                                 
